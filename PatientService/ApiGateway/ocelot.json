{
  "Routes": [
    {
      "DownstreamPathTemplate": "/swagger",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "apigateway",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/Patients/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "patientapi",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/patient/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "PatientService"
    },
    {
      "DownstreamPathTemplate": "/api/identity/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "identityservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/identity/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "IdentityService"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "PatientService",
      "Config": [
        {
          "Name": "Patient API",
          "Version": "v1",
          "Url": "http://patientapi:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "IdentityService",
      "Config": [
        {
          "Name": "Identity API",
          "Version": "v1",
          "Url": "http://identityservice:8080/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
