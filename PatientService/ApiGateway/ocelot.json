{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "identityservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/auth/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/patient/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "patientapi",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/api/patient/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "IdentityService",
      "Config": [
        {
          "Name": "Identity API",
          "Version": "v1",
          "Url": "http://identityservice:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "PatientService",
      "Config": [
        {
          "Name": "Patient API",
          "Version": "v1",
          "Url": "http://patientapi:8080/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
